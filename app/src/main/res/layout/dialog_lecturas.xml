<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:layout_gravity="center"
    android:layout_marginStart="10dp"
    android:layout_marginEnd="10dp">

    <!-- Falla -->
    <LinearLayout
        android:layout_weight="1"
        android:layout_marginTop="6dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:background="@color/verdePastel"
                android:fontFamily="@font/gotham_medium"
                android:gravity="center"
                android:text="Falla"
                android:textColor="@color/blanco"
                android:textSize="15sp"
                android:layout_marginBottom="10dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                 <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_gravity="center"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/icon_camera_falla"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/icon_cam2"
                            android:layout_gravity="center"/>

                        <ImageView
                            android:id="@+id/icon_gallery_falla"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_galery"
                            android:visibility="gone"/>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <!-- Temperatura -->
    <LinearLayout
        android:layout_weight="1"
        android:layout_marginTop="6dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:background="@color/verdePastel"
                android:fontFamily="@font/gotham_medium"
                android:gravity="center"
                android:text="Temperatura (째C)"
                android:textColor="@color/blanco"
                android:textSize="15sp"
                android:layout_marginBottom="10dp"/>
            <!-- Lineas Temperaturas -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <!-- L1 -->
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="T(Serpentin)"
                        android:gravity="center"
                        android:fontFamily="@font/gotham_medium"
                        android:textColor="@color/negro"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="horizontal">
                        <EditText
                            android:id="@+id/et_temperatura_l1"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="0"
                            android:inputType="numberDecimal"
                            android:gravity="center"/>
                        <TextView
                            android:text="째C"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textSize="16sp"
                            android:layout_gravity="center"
                            android:fontFamily="@font/gotham_medium"
                            android:visibility="gone"/>
                        <ImageView
                            android:id="@+id/icon_cam_temp_l1"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/icon_cam2"
                            android:layout_gravity="center"/>

                        <ImageView
                            android:id="@+id/icon_gallery_temp_l1"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_galery"
                            android:visibility="gone"/>

                    </LinearLayout>

                </LinearLayout>
                <!-- L2 -->
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="T(Ambiente)"
                        android:gravity="center"
                        android:fontFamily="@font/gotham_medium"
                        android:textColor="@color/negro"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:orientation="horizontal">
                        <EditText
                            android:id="@+id/et_temperatura_l2"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="0"
                            android:inputType="numberDecimal"
                            android:gravity="center"/>
                        <TextView
                            android:text="째C"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textSize="16sp"
                            android:layout_gravity="center"
                            android:fontFamily="@font/gotham_medium"
                            android:visibility="gone"/>
                        <ImageView
                            android:id="@+id/icon_cam_temp_l2"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_cam2"/>

                        <ImageView
                            android:id="@+id/icon_gallery_temp_l2"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_galery"
                            android:visibility="gone"/>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <!-- Presi처n -->
    <LinearLayout
        android:layout_weight="1"
        android:layout_marginTop="6dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:background="@color/verdePastel"
                android:fontFamily="@font/gotham_medium"
                android:gravity="center"
                android:text="Presi처n (PSI)"
                android:textColor="@color/blanco"
                android:textSize="15sp"
                android:layout_marginBottom="10dp"/>
            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <!-- Baja -->
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Baja"
                        android:fontFamily="@font/gotham_medium"
                        android:gravity="center"
                        android:textSize="15sp"
                        android:textColor="@color/negro"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_presion_baja"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="0"
                            android:inputType="numberSigned"
                            android:gravity="center"
                            android:textSize="15sp"/>

                        <TextView
                            android:text="PSI"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textSize="16sp"
                            android:layout_gravity="center"
                            android:fontFamily="@font/gotham_medium"
                            android:visibility="gone"/>


                        <ImageView
                            android:id="@+id/icon_camera_presion_baja"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_cam2" />

                        <ImageView
                            android:id="@+id/icon_gallery_presion_baja"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_galery"
                            android:visibility="gone"/>
                    </LinearLayout>

                </LinearLayout>
                <!-- Alta -->
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Alta"
                        android:fontFamily="@font/gotham_medium"
                        android:gravity="center"
                        android:textSize="15sp"
                        android:textColor="@color/negro"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_presion_alta"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="0"
                            android:inputType="numberSigned"
                            android:gravity="center"
                            android:textSize="15sp"/>

                        <TextView
                            android:text="PSI"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textSize="16sp"
                            android:layout_gravity="center"
                            android:fontFamily="@font/gotham_medium"
                            android:visibility="gone"/>

                        <ImageView
                            android:id="@+id/icon_camera_presion_alta"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_cam2" />

                        <ImageView
                            android:id="@+id/icon_gallery_presion_alta"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_galery"
                            android:visibility="gone"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <!-- El챕ctrica -->
    <LinearLayout
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:paddingBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingBottom="10dp"
            android:layout_marginBottom="10dp">

            <TextView
                android:text="Energ챠a"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:background="@color/verdePastel"
                android:fontFamily="@font/gotham_medium"
                android:gravity="center"
                android:textColor="@color/blanco"
                android:textSize="15sp"
                android:visibility="gone"/>

            <TextView
                android:text="Amperaje (A)"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:background="@color/verdePastel"
                android:fontFamily="@font/gotham_medium"
                android:gravity="center"
                android:textColor="@color/blanco"
                android:textSize="15sp"/>

            <LinearLayout
                android:layout_weight="0.5"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="vertical">
                <!-- Amperaje -->
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">
                        <!-- Lineas Amperaje -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">
                            <!-- L1 -->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:text="L1"
                                    android:gravity="center"
                                    android:fontFamily="@font/gotham_medium"
                                    android:textColor="@color/negro"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:orientation="horizontal">
                                    <EditText
                                        android:id="@+id/et_amperaje_l1"
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="0"
                                        android:inputType="numberDecimal"
                                        android:gravity="center"/>
                                    
                                    <TextView
                                        android:text="A"
                                        android:layout_width="27dp"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:textSize="16sp"
                                        android:layout_gravity="center"
                                        android:fontFamily="@font/gotham_medium"
                                        android:visibility="gone"/>
                                    <ImageView
                                        android:id="@+id/icon_cam_amp_l1"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_cam2"/>

                                    <ImageView
                                        android:id="@+id/icon_gallery_amp_l1"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_galery"
                                        android:visibility="gone"/>

                                </LinearLayout>

                            </LinearLayout>
                            <!-- L2 -->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:text="L2"
                                    android:gravity="center"
                                    android:fontFamily="@font/gotham_medium"
                                    android:textColor="@color/negro"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:orientation="horizontal">
                                    <EditText
                                        android:id="@+id/et_amperaje_l2"
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="0"
                                        android:inputType="numberDecimal"
                                        android:gravity="center"/>

                                    <TextView
                                        android:text="A"
                                        android:layout_width="27dp"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:textSize="16sp"
                                        android:layout_gravity="center"
                                        android:fontFamily="@font/gotham_medium"
                                        android:visibility="gone"/>

                                    <ImageView
                                        android:id="@+id/icon_cam_amp_l2"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_cam2"/>

                                    <ImageView
                                        android:id="@+id/icon_gallery_amp_l2"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_galery"
                                        android:visibility="gone"/>

                                </LinearLayout>

                            </LinearLayout>
                            <!-- L3 -->
                            <LinearLayout
                                android:id="@+id/linear_amp_l3"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:text="L3"
                                    android:gravity="center"
                                    android:fontFamily="@font/gotham_medium"
                                    android:textColor="@color/negro"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:orientation="horizontal">
                                    <EditText
                                        android:id="@+id/et_amperaje_l3"
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="0"
                                        android:inputType="numberDecimal"
                                        android:gravity="center"/>

                                    <TextView
                                        android:layout_width="27dp"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center"
                                        android:fontFamily="@font/gotham_medium"
                                        android:gravity="center"
                                        android:text="A"
                                        android:textSize="16sp"
                                        android:visibility="gone"/>
                                    <ImageView
                                        android:id="@+id/icon_cam_amp_l3"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_cam2"/>

                                    <ImageView
                                        android:id="@+id/icon_gallery_amp_l3"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_galery"
                                        android:visibility="gone"/>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <!-- Voltaje -->
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_marginTop="4dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:text="Voltaje (V)"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        android:background="@color/verdePastel"
                        android:fontFamily="@font/gotham_medium"
                        android:gravity="center"
                        android:textColor="@color/blanco"
                        android:textSize="15sp"/>
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">
                        <!-- Lineas Amperaje -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">
                            <!-- L1 -->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:text="L1"
                                    android:gravity="center"
                                    android:fontFamily="@font/gotham_medium"
                                    android:textColor="@color/negro"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:orientation="horizontal">
                                    <EditText
                                        android:id="@+id/et_voltaje_l1"
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="0"
                                        android:inputType="numberDecimal"
                                        android:gravity="center"/>

                                    <ImageView
                                        android:id="@+id/icon_cam_volt_l1"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_cam2"/>

                                    <ImageView
                                        android:id="@+id/icon_gallery_volt_l1"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_galery"
                                        android:visibility="gone"/>

                                </LinearLayout>

                            </LinearLayout>
                            <!-- L2 -->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:text="L2"
                                    android:gravity="center"
                                    android:fontFamily="@font/gotham_medium"
                                    android:textColor="@color/negro"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:orientation="horizontal">
                                    <EditText
                                        android:id="@+id/et_voltaje_l2"
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="0"
                                        android:inputType="numberDecimal"
                                        android:gravity="center"/>
                                    <TextView
                                        android:text="V"
                                        android:layout_width="27dp"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:textSize="16sp"
                                        android:layout_gravity="center"
                                        android:fontFamily="@font/gotham_medium"
                                        android:visibility="gone"/>
                                    <ImageView
                                        android:id="@+id/icon_cam_volt_l2"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_cam2"/>

                                    <ImageView
                                        android:id="@+id/icon_gallery_volt_l2"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_galery"
                                        android:visibility="gone"/>

                                </LinearLayout>

                            </LinearLayout>
                            <!-- L3 -->
                            <LinearLayout
                                android:id="@+id/linear_volt_l3"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:text="L3"
                                    android:gravity="center"
                                    android:fontFamily="@font/gotham_medium"
                                    android:textColor="@color/negro"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:orientation="horizontal">
                                    <EditText
                                        android:id="@+id/et_voltaje_l3"
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="0"
                                        android:inputType="numberDecimal"
                                        android:gravity="center"/>
                                    <TextView
                                        android:text="V"
                                        android:layout_width="27dp"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:textSize="16sp"
                                        android:layout_gravity="center"
                                        android:fontFamily="@font/gotham_medium"
                                        android:visibility="gone"/>

                                    <ImageView
                                        android:id="@+id/icon_cam_volt_l3"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_cam2" />

                                    <ImageView
                                        android:id="@+id/icon_gallery_volt_l3"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/icon_galery"
                                        android:visibility="gone"/>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="6dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/dialog_lectura_btn_cancelar"
            android:text="Cancelar"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
